{
  "manifest_version": 3,
  "name": "Dino Modder",
  "version": "1.2",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/img/icon16.png",
      "48": "/img/icon48.png"
    }
  },
  "icons": {
    "16": "/img/icon16.png",
    "48": "/img/icon48.png"
  }
}
